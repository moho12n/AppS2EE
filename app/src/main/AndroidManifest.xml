<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.s2ee.etic.apps2ee">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoff"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logoff"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EnstrepriseActivity" />
        <activity android:name=".ShowActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".AgendaActivity" />
        <activity android:name=".MapActivity" />
        <activity android:name=".AboutusActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FavouritActivity" />
        <activity android:name=".SendActivity"></activity>
    </application>

</manifest>